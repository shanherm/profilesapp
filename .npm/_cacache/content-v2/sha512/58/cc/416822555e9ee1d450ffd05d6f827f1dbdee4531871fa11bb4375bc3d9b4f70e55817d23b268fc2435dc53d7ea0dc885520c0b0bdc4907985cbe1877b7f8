ry for inspiration and implementation reference).\n","readmeFilename":"README.md","gitHead":"0e51a1490b4b36b819c088c87841f38a863e69c4","bugs":{"url":"https://github.com/ardatan/graphql-tools/issues"},"homepage":"https://github.com/ardatan/graphql-tools#readme","_id":"@graphql-tools/optimize@2.0.0-alpha-20230518210711-0e51a149","_nodeVersion":"18.16.0","_npmVersion":"9.5.1","dist":{"integrity":"sha512-QXoZdVCE9OTLa1vr4UfyBi/vbypBMI38AmRUWG4yKvEUVr4VArGAIJwzfbGQLrpkBEB70dCUD0iS4JfA1Je9Ow==","shasum":"82a9eb1c29abc657b32c5193c42064aad4f340f8","tarball":"https://registry.npmjs.org/@graphql-tools/optimize/-/optimize-2.0.0-alpha-20230518210711-0e51a149.tgz","fileCount":27,"unpackedSize":16195,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEMCH1AEZivslEyNn3uQu58W0FMpze/omxhg2l+eKnceaH8CIEiyQtr3NDe7xtvBfEWfI9mjz7vc2+eW1gNU7Ob/PXLN"}]},"_npmUser":{"name":"ardatan","email":"ardatanrikulu@gmail.com"},"directories":{},"maintainers":[{"name":"dotansimha","email":"dotansimha@gmail.com"},{"name":"urigo","email":"uri.goldshtein@gmail.com"},{"name":"ardatan","email":"ardatanrikulu@gmail.com"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/optimize_2.0.0-alpha-20230518210711-0e51a149_1684444106681_0.30483915832127817"},"_hasShrinkwrap":false},"2.0.0-alpha-20230519000249-b153667f":{"name":"@graphql-tools/optimize","version":"2.0.0-alpha-20230519000249-b153667f","description":"A set of utils for faster development of GraphQL tools","sideEffects":false,"peerDependencies":{"graphql":"^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0"},"dependencies":{"tslib":"^2.4.0"},"repository":{"type":"git","url":"git+https://github.com/ardatan/graphql-tools.git","directory":"packages/optimize"},"license":"MIT","engines":{"node":">=16.0.0"},"main":"cjs/index.js","module":"esm/index.js","typings":"typings/index.d.ts","typescript":{"definition":"typings/index.d.ts"},"type":"module","exports":{".":{"require":{"types":"./typings/index.d.cts","defaul